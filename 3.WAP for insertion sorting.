#include<iostream>
using namespace std;
void insertion(int arr[],int n){
    for (int i = 0; i <= n - 1; i++) {
        int j = i;
        while (j > 0 && arr[j - 1] > arr[j]) {
            int temp = arr[j - 1];
            arr[j - 1] = arr[j];
            arr[j] = temp;
            j--;
    }
}
}
int main()
{
    int n;
    cout << "Enter the number:";
    cin >> n;
    int arr[n];
    cout << "enter " << n << " number in array:\n";
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }
    cout << "Befor sorting:\n";
    for(int i=0;i<n;i++){
        cout << arr[i] << " ";
    }
    insertion(arr,n);
    cout << "\nafter sorting.\n";
    for(int i=0;i<n;i++){
        cout << arr[i] << " ";
    }
    return 0;
}
